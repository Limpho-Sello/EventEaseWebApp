<
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - EventEaseWebApp</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" />
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark mb-4">
        <div class="container-fluid">
            <a class="navbar-brand" href="/">EventEaseWebApp</a>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item"><a class="nav-link" asp-controller="Venue" asp-action="Index">Venues</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Booking" asp-action="Index">Bookings</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Event" asp-action="Index">Events</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <main role="main">
        @RenderBody()
    </main>
    <div>
    <footer class="mt-5">
        <hr />
        <p>&copy; @DateTime.Now.Year - EventEase</p>
    </footer>
    </div>

    @RenderSection("Scripts", required: false)

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate-unobtrusive/4.0.0/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>

   
  
